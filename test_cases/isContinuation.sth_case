[default]
DRIVER=java_subroutine
OPTIONS="-L ."
ENTRY=isContinuation

[case]
ARGS="0"
EXIT_STATUS=1

[case]
ARGS="0x80" 
EXIT_STATUS=3 

[case]
ARGS="0xF7" 
EXIT_STATUS=1

[case]
ARGS="0x7FF"
EXIT_STATUS=2

[case]
ARGS="0xFFFF"
EXIT_STATUS=3

[case]
ARGS="0x10FFFF"
EXIT_STATUS=4

[case]
ARGS="0x0080"
EXIT_STATUS=2
