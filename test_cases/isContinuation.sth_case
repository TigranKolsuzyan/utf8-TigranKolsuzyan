[default]
DRIVER=java_subroutine
OPTIONS="-L ."
ENTRY=isContinuation

[case]
ARGS="0"
OUTPUT=1
EXIT_STATUS=1

[case]
ARGS="0x80" 
OUTPUT=0
EXIT_STATUS=0 

[case]
ARGS="0xF7"
OUTPUT=1 
EXIT_STATUS=1

[case]
ARGS="0x7FF"
OUTPUT=1
EXIT_STATUS=1

[case]
ARGS="0xFFFF"
OUTPUT=1
EXIT_STATUS=1

[case]
ARGS="0x10FFFF"
OUTPUT=1
EXIT_STATUS=1

[case]
ARGS="0x0080"
OUTPUT=0
EXIT_STATUS=0
